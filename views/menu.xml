    <menuitem 
        id="rental_order_menu_categ" 
        name="Order" 
        parent="rental_menu_root" 
        sequence="20"/>

    <!-- This Menu Item Must have a parent -->
    <menuitem id="rental_partner_menu_categ" name="Partner" parent="rental_menu_root" sequence="30"/>
</odoo>

        id="rental_menu_root"
        name="Rental KM2"
        sequence="10"/>

    <!-- This Menu Item Must have a parent -->
    <menuitem
        id="rental_menu_categ"
        name="Etalase"
        parent="rental_menu_root"
        sequence="10"/>
